{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-underscore-dangle": 0,
      "max-classes-per-file": 0,
      "no-console": 0,
      "object-curly-newline": 0,
      "class-methods-use-this": 0,
      "prefer-promise-reject-errors": 0
    }
}